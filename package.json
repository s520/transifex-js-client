{
  "name": "transifex-js-client",
  "version": "1.1.0",
  "description": "Wrapper around Transifex's API on node and Browser",
  "main": "src/index.js",
  "browser": "dist/transifex-api.min.js",
  "repository": "git@github.com:transifex/transifex-js-client.git",
  "author": "Alex Psi <alexpsi256@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "test": "rimraf coverage && karma start",
    "jsdoc": "node_modules/jsdoc/jsdoc.js -r src/ -d docs --readme readme.md",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel lint build test",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.9",
    "cpy-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-eslint": "^6.0.0",
    "eslint-plugin-jsdoc": "^22.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jsdoc": "^3.6.3",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "opt-cli": "^1.6.0",
    "phantomjs-polyfill": "^0.0.2",
    "rimraf": "^3.0.2",
    "shortid": "^2.2.15",
    "webpack": "^4.42.0",
    "webpack-config-utils": "^2.3.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "blueimp-md5": "^2.12.0",
    "extend": "^3.0.2"
  }
}
